<table>
  <tr>
    <td>ID</td>
    <td>Name</td>
    <td>Lastname</td>
    <td>Email</td>
  </tr>

  @foreach (var user in Model)
  {
    <tr>
      <td>@user.id_user</td>
      <td>@user.name_user</td>
      <td>@user.lastname</td>
      <td>@user.email</td>
      <td>@user.created_at</td>
    </tr>
  }
</table>